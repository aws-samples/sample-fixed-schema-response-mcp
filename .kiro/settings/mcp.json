{
  "mcpServers": {
    "fixed-schema": {
      "command": "uv",
      "args": [
        "run",
        "--directory",
        "/Users/fanhongy/Project/mcps-proj/fixed_schema_mcp_server",
        "fastmcp_server.py"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "INFO",
        "AWS_PROFILE": "default",
        "AWS_REGION": "us-west-2"
      },
      "disabled": false,
      "autoApprove": [
        "list_available_schemas",
        "add_schema",
        "get_troubleshooting_guide",
        "get_recipe",
        "get_movie_review",
        "get_weather_report",
        "get_product_info",
        "get_book_review",
        "get_article_summary",
        "get_person_profile"
      ]
    },
    "aws-docs": {
      "command": "/opt/homebrew/bin/uvx",
      "args": [
        "awslabs.aws-documentation-mcp-server@latest"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false,
      "autoApprove": []
    }
  }
}